{% extends "base.html.twig" %}

{% block title %}About{% endblock %}

{% block body %}
<h1>Session Data</h1>
    <pre>
        {{ sessionData | json_encode(constant('JSON_PRETTY_PRINT')) }}
    </pre>
    {% for message in app.flashes('success') %}
    <div class="flash-notice">
        {{ message }}
    </div>
{% endfor %}

{% endblock %}